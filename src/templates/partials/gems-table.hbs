<tbody id="gems-table-body"> 
  {{#each CATALOGUE_DATA}}
    <tr>
      <td>{{gemId}}</td> 
      <td>{{gemType}}</td>
      <td>{{carats}}</td>
      <td>{{cut}}</td>
      <td>{{color}}</td>
      <td><a href="./gem/{{gemId}}"><img class="img-responsive thumbnail img-rounded" src="./img/Gem{{gemId}}/Gem{{gemId}}_thumb.jpg"/>{{color}} {{gemType}}</a></td>
    </tr>
  {{/each}}
</tbody>